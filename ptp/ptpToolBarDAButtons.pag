Private Function GetButtonWithIndex(nIndex As Long) As ToolBarDAButton
    Dim c As Long
    
    For c = 1 To mButtons.Count
        If mButtons(c).Index = nIndex Then
            Set GetButtonWithIndex = mButtons(c)
            Exit For
        End If
    Next c
    
End Function
    
Private Sub FixOrderToHide()
    Dim c As Long
    
    For c = 1 To mButtons.Count
        If mButtons(c).OrderToHide > mButtons.Count Then
            DoFixOrderToHide
            Exit Sub
        End If
    Next c
End Sub

Private Sub DoFixOrderToHide()
    Dim c As Long
    Dim iGreatest As Long
    Dim c2 As Long
    Dim iOTH_Prev() As Long
    Dim iOTH_New() As Long
    Dim iNewOrder As Long
    
    iGreatest = mButtons(1).OrderToHide
    For c = 2 To mButtons.Count
        If mButtons(c).OrderToHide > iGreatest Then
            iGreatest = mButtons(c).OrderToHide
        End If
    Next c
    
    ReDim iOTH_Prev(0)
    For c = iGreatest To 1 Step -1
        For c2 = 1 To mButtons.Count
            If mButtons(c2).OrderToHide = c Then
                ReDim Preserve iOTH_Prev(UBound(iOTH_Prev) + 1)
                iOTH_Prev(UBound(iOTH_Prev)) = c
                Exit For
            End If
        Next c2
    Next c
    
    If UBound(iOTH_Prev) > 0 Then
        ReDim iOTH_New(UBound(iOTH_Prev))
        iNewOrder = mButtons.Count
        iOTH_New(1) = iNewOrder
        For c = 2 To UBound(iOTH_Prev)
            If iOTH_Prev(c) >= iNewOrder Then
                iNewOrder = iNewOrder - 1
                iOTH_New(c) = iNewOrder
            Else
                iOTH_New(c) = iOTH_Prev(c)
            End If
        Next c
        
        For c = 1 To UBound(iOTH_Prev)
            For c2 = 1 To mButtons.Count
                If mButtons(c2).OrderToHide = iOTH_Prev(c) Then
                   mButtons(c2).OrderToHide = iOTH_New(c)
                End If
            Next c2
        Next c
    End If
End Sub

