Private Sub cmdBrowse16_Click()
    Dim iDlg As New CommonDialogExObject
    
    iDlg.Filter = "Bitmap files|*.bmp"
    iDlg.ShowOpen
    If Not iDlg.Canceled Then
        Changed = True
        On Error Resume Next
        Set picImg16.Picture = LoadPicture(iDlg.FileName)
        If Err.Number <> 0 Then
            MsgBox "Error: " & Err.Number & ", " & Err.Description, vbCritical
            On Error GoTo 0
        End If
        On Error GoTo 0
        UpdateData
        CheckImageSizes
    End If
End Sub

