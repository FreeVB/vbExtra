Private Sub PropertyPage_ApplyChanges()
    Dim iButtons As ToolBarDAButtons
    Dim iButton As ToolBarDAButton
    Dim c As Long
    Dim c2 As Long
    Dim iTb As ToolBarDA
    
    FixOrderToHide
    Set iButtons = New ToolBarDAButtons
    
    For c = 1 To mButtons.Count
        For c2 = 1 To mButtons.Count
            If mButtons(c2).Index = c Then
                Set iButton = mButtons(c2).Clone
                iButtons.AddButtonObject iButton
