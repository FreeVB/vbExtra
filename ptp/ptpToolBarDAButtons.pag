    For c = 1 To mButtons.Count
        If Not mButtons(c).Pic30Alt Is Nothing Then
            iStr = Round(PropertyPage.ScaleX(mButtons(c).Pic30Alt.Width, vbHimetric, vbPixels)) & "x" & Round(PropertyPage.ScaleY(mButtons(c).Pic30Alt.Height, vbHimetric, vbPixels))
            If iStrImgSize = "" Then
                iStrImgSize = iStr
                iStrImgSize_Button = c
            ElseIf iStr <> iStrImgSize Then
                mStrImagesDifferences = mStrImagesDifferences & "The Image size of the property 'Pic30Alt' in Button " & CStr(iStrImgSize_Button) & " is " & iStrImgSize & " and in Button " & CStr(c) & " it is " & iStr & "." & vbCrLf
                Exit For
            End If
        End If
    Next c
    
    iStrImgSize = ""
    For c = 1 To mButtons.Count
        If Not mButtons(c).Pic36Alt Is Nothing Then
            iStr = Round(PropertyPage.ScaleX(mButtons(c).Pic36Alt.Width, vbHimetric, vbPixels)) & "x" & Round(PropertyPage.ScaleY(mButtons(c).Pic36Alt.Height, vbHimetric, vbPixels))
            If iStrImgSize = "" Then
                iStrImgSize = iStr
                iStrImgSize_Button = c
            ElseIf iStr <> iStrImgSize Then
                mStrImagesDifferences = mStrImagesDifferences & "The Image size of the property 'Pic36Alt' in Button " & CStr(iStrImgSize_Button) & " is " & iStrImgSize & " and in Button " & CStr(c) & " it is " & iStr & "." & vbCrLf
                Exit For
            End If
        End If
    Next c
    
    
    If mStrImagesDifferences <> "" Then
        mStrImagesDifferences = mStrImagesDifferences & vbCrLf & "(There might be other images that have different sizes besides these ones)."
        lblWarningImgSizes.Visible = True
    Else
        lblWarningImgSizes.Visible = False
    End If
    
End Sub

Private Sub txtToolTipText_GotFocus()
    If txtToolTipText.SelStart = 0 Then txtToolTipText.SelStart = Len(txtToolTipText.Text)
End Sub
