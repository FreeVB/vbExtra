Private Sub cmdDeleteButton_Click()
    Dim c As Long
    
    For c = 1 To mButtons.Count
        If mButtons(c).Index = mCurrentButtonIndex Then
            mButtons.Remove (c)
            Exit For
        End If
    Next c
    For c = 1 To mButtons.Count
        If mButtons(c).Index > mCurrentButtonIndex Then
            mButtons(c).Index = mButtons(c).Index - 1
        End If
    Next c
    
    Changed = True
    
    If mCurrentButtonIndex > mButtons.Count Then mCurrentButtonIndex = mButtons.Count
    
    ShowCurrentButtonData
    
End Sub

