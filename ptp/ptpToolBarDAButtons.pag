Private Sub PropertyPage_SelectionChanged()
    Dim iTBB As ToolBarDAButton
    Dim iButton As ToolBarDAButton
    
    SetTextBoxNumeric txtPlaceHolderWidth
    SetTextBoxNumeric txtOrderToHide
    cboStyle.Clear
    cboStyle.AddItem "0 - vxTBDefault"
    cboStyle.AddItem "1 - vxTBCheck"
    cboStyle.AddItem "2 - vxTBButtonGroup"
    cboStyle.AddItem "3 - vxTBSeparator"
    cboStyle.AddItem "4 - vxTBPlaceholder"
    
    Set mButtons = New ToolBarDAButtons
    
    For Each iTBB In SelectedControls(0).Buttons
        Set iButton = iTBB.Clone
        mButtons.AddButtonObject iButton
    Next
    
    If mButtons.Count > 0 Then
        If mCurrentButtonIndex = 0 Then
            mCurrentButtonIndex = 1
        End If
    End If
    ShowCurrentButtonData
    CheckImageSizes
    
    cmdPaste.Enabled = Not gToolBarDAButtonCopied Is Nothing
