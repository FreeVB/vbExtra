Private Sub cmdPaste_Click()
    Dim iButton As ToolBarDAButton
    Dim c As Long
    Dim iIndex As Long
    
    If MsgBox("It will replace any current data on this button, continue?", vbYesNo) = vbNo Then Exit Sub
    
    If Not gToolBarDAButtonCopied Is Nothing Then
        Set iButton = gToolBarDAButtonCopied.Clone
        iButton.Index = mCurrentButtonIndex
        
        For c = 1 To mButtons.Count
            If mButtons(c).Index = mCurrentButtonIndex Then
                iIndex = c
                Exit For
            End If
        Next c
        
        mButtons.AddButtonObject iButton, , , iIndex
        mButtons.Remove iIndex
        ShowCurrentButtonData
        Changed = True
    End If
End Sub

